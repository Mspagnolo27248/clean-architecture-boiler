{
"version": "0.2.0",
"configurations": [
    
    {
        "type": "node",
        "request": "launch",
        "name": "Typscript Debug",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "program": "${workspaceFolder}\\src\\server.ts",
        "runtimeArgs": ["-r","ts-node/register","-r","tsconfig-paths/register"],
        "outFiles": [
            "${workspaceFolder}/**/*.js"
        ]
    },

    {
        "type": "node",
        "request": "launch",
        "name": "Perplexity",
        "skipFiles": [
            "<node_internals>/**"
        ],
        "program": "${workspaceFolder}/src/server.ts",
        "runtimeArgs": [
            "-r",
            "ts-node/register",
            "-r",
            "tsconfig-paths/register"
        ],
       
    }
]
}



  
